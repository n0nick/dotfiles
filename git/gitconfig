[rebase]
	autosquash = true
[push]
	default = simple
[core]
	editor = vim -f
[instaweb]
	httpd = webrick
	port = 4000
[alias]
	a = add
	aa = add --all
	ap = add -p
	b = branch
	bv = branch -vv
	bd = branch -D
	bm = branch -m
	br = branch -r
	cf = commit --amend --no-edit
	ci = commit
	cp = cherry-pick
	cm = commit -e -m
	cia = commit --amend
	cb = checkout -b
	co = checkout
	d = diff
	dc = diff --cached
	di = diff
	f = fetch
	h = help
	l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr by %C(yellow)%an%Creset)%Creset'
	m = merge
	mff = merge --no-ff
	pl = pull
	ps = push
	pst = push --tags
	pullff = pull --ff-only
	r = reset
	rb = rebase
	rbc = rebase --continue
	rbs = rebase --skip
	rbi = rebase -i
	rh = reset HEAD
	s = status
	sl = stash list
	st = stash
	sp = stash pop
	ss = stash show -p
	t = tag
	semtag = !git tag -a $(semver tag) -m \"tagging $(semver tag)\"
[include]
	path = .gitconfig.user
